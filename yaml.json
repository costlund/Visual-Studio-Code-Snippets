{
	"snippet_html": {
		"prefix": "snippet_html",
		"body": [
      "settings:",
      "  path: 'html/innerHTML/body/innerHTML'",
      "content:",
      "  doctype:",
      "    type: text",
      "    text: '<!DOCTYPE html>'",
      "  html:",
      "    type: html",
      "    innerHTML:",
      "      head:",
      "        type: head",
      "        innerHTML:",
      "          -",
      "            type: title",
      "            innerHTML: yml:/theme/dev/theme/config/data.yml:title",
      "          -",
      "            type: widget",
      "            data:",
      "              plugin: 'theme/include'",
      "              method: include",
      "              data:",
      "                bootswatch:",
      "                  theme: Journal",
      "      body:",
      "        type: body",
      "        innerHTML: _body_to_replace_"
		],
		"description": "."
	},
	"snippet_a": {
		"prefix": "snippet_a",
		"body": [
		  "-",
		  "  type: a",
		  "  attribute: ",
		  "    class: btn btn-primary",
		  "  innerHTML: $1"
		],
		"description": "."
	},
	"snippet_a_primary": {
		"prefix": "snippet_a_primary",
		"body": [
      "-",
		  "  type: a",
		  "  attribute: ",
		  "    class: btn btn-primary",
		  "  innerHTML: $1"
		],
		"description": "."
	},
	"snippet_p": {
		"prefix": "snippet_p",
		"body": [
		  "-",
		  "  type: p",
		  "  innerHTML: $1"
		],
		"description": "."
	},
	"snippet_div": {
		"prefix": "snippet_div",
		"body": [
		  "-",
		  "  type: div",
		  "  innerHTML: $1"
		],
		"description": "."
	},
	"snippet_div_alert_success": {
		"prefix": "snippet_div_alert_success",
		"body": [
		  "-",
		  "  type: div",
		  "  attribute: ",
		  "    class: alert alert-success",
		  "  innerHTML: $1"
		],
		"description": "."
	},
	"snippet_div_container": {
		"prefix": "snippet_div_container",
		"body": [
		  "-",
		  "  type: div",
		  "  attribute: ",
		  "    class: container",
		  "  innerHTML: $1"
		],
		"description": "."
	},
	"snippet_div_container-fluid": {
		"prefix": "snippet_div_container-fluid",
		"body": [
		  "-",
		  "  type: div",
		  "  attribute: ",
		  "    class: container-fluid",
		  "  innerHTML: $1"
		],
		"description": "."
	},
	"snippet_div_row": {
		"prefix": "snippet_div_row",
		"body": [
		  "-",
		  "  type: div",
		  "  attribute: ",
		  "    class: row",
		  "  innerHTML: $1"
		],
		"description": "."
	},
	"snippet_div_col_12": {
		"prefix": "snippet_div_col_12",
		"body": [
		  "-",
		  "  type: div",
		  "  attribute: ",
		  "    class: col-md-12",
		  "  innerHTML: $1"
		],
		"description": "."
	},
	"snippet_div_strong_span": {
		"prefix": "snippet_div_strong_span",
		"body": [
		  "-",
		  "  type: div",
		  "  innerHTML:",
		  "    - ",
		  "      type: strong",
		  "      attribute:",
		  "        class: content-colon",
		  "      innerHTML: $1",
		  "    - ",
		  "      type: span",
		  "      innerHTML: $2"
		],
		"description": "."
	},
	"snippet_img_thumbnail": {
		"prefix": "snippet_img_thumbnail",
		"body": [
		  "-",
		  "  type: img",
		  "  attribute: ",
		  "    src: /path/test.jpg"
		],
		"description": "."
	},
	"snippet_script": {
		"prefix": "snippet_script",
		"body": [
		  "-",
		  "  type: script",
		  "  innerHTML: $1"
		],
		"description": "."
	},
	"snippet_widget": {
		"prefix": "snippet_widget",
		"body": [
		  "-",
		  "  type: widget",
		  "  data:",
		  "    plugin: $1",
		  "    method: $2",
		  "    data:",
		  "      x: $3",
		],
		"description": "."
	},
	"snippet_manifest_version": {
		"prefix": "snippet_manifest_version",
		"body": [
		  "1.0.0:",
		  "  date: '2023-$1'",
		  "  title: $2",
		  "  description: '$3.'",
		],
		"description": "."
	},
  "snippet_a_list_group": {
		"prefix": "snippet_a_list_group",
		"body": [
		  "-",
		  "  type: div",
		  "  attribute:",
		  "    class: list-group",
		  "    style:",
		  "        margin-bottom: 20px",
		  "  innerHTML:",
		  "    -",
		  "      type: a",
		  "      attribute:",
		  "        class: list-group-item",
		  "        href: $1",
		  "      innerHTML: $2"
		],
		"description": "."
	}
}